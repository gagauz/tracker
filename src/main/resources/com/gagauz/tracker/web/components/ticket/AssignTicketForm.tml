<div xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd" xmlns:p="tapestry:parameter">
	<div t:id="zone">
		<t:beanform t:id="form" object="assign" cancel="true" add="owner" include="comment" reorder="owner,comment" zone="zone">
			<p:owner>
				<t:label class="col-sm-3 control-label" for="owner" />
				<div class="col-sm-9">
					<t:UserAutocompleteField t:id="owner" t:user="assign.toOwner" />
					<t:error for="owner"/>
					<t:hidden value="ticket" />
				</div>
			</p:owner>
		</t:beanform>
	</div>
</div>
