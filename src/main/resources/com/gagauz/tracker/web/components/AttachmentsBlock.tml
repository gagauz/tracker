<div class="ticket-header" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">
     
	<t:loop source="attachments" value="attachment">
   		<a href="#" onclick="jQuery(this).modal('show');return false;">
    		<t:if test="attachment.image">
    			<img src="${attachment.value}" />
    			<p:else>
    			${attachment.value}
    			</p:else>
    		</t:if>
   		</a>
   	</t:loop>
</div>