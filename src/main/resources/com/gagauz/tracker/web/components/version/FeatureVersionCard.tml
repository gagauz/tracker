<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
	xmlns:p="tapestry:parameter">
	<div class="feature-version-card">
		<div class="head">
			<t:if test="canCreateTicket">
				<t:ModalLink role="button" class="bt" t:zone="prop:ticketZone"
					t:action="createTicket" t:context="[feature, version]" title="${message:create-task}">
					<i class="glyphicon glyphicon-plus"></i>
				</t:ModalLink>
			</t:if>
		</div>
	</div>
</t:container>