<html t:type="layout" title="Contact com.gagauz"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

    <h2>Login page</h2>
    <t:form t:id="loginForm">
    	<t:label for="username"></t:label>
    	<t:textfield t:id="username" value="username"/><br/>
    	<t:label for="password"></t:label>
    	<t:passwordfield t:id="password" value="password"/><br/>
    	<t:label for="remember"></t:label>
    	<t:checkbox t:id="remember" value="remember"/><br/>
    	<t:submit/>
    </t:form>

</html>
