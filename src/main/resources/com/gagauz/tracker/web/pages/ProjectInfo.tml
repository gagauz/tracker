<html t:type="layout" title="${project.name}"
      t:sidebarTitle="Framework Version"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

	
	<h3>Project roles <t:pagelink page="rolegrouplist" context="[project,null]">Manage</t:pagelink></h3>
	<t:grid source="roleGroups" row="roleGroup" add="edit">
    	<p:editCell>
    		<t:pagelink page="rolegrouplist" context="[project, roleGroup]">edit</t:pagelink>
    	</p:editCell>
    </t:grid>

	<hr/>
    
    
    <t:ProjectMap project="project"/>

</html>
